{
  "name": "issue-creator",
  "version": "0.1.0",
  "description": "A Slack slash command for creating quickly a new issue.",
  "main": "src/index.js",
  "scripts": {
    "test": "jest --coverage",
    "lint": "eslint src/**",
    "config": "node ./scripts/configure.js",
    "deconfig": "node ./scripts/deconfigure.js"
  },
  "config": {
    "s3BucketName": "YOUR_UNIQUE_BUCKET_NAME",

  "bugs": {
    "url": "https://github.com/kentaro-m/issue-creator/issues"
  },
  "homepage": "https://github.com/kentaro-m/issue-creator#readme",
  "devDependencies": {
    "@slack/client": "^4.9.0",
    "eslint": "^5.14.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-standard": "^3.1.0",
    "jest": "^23.6.0"
  },
  "dependencies": {
    "@octokit/rest": "^15.10.0",
    "aws-serverless-express": "^3.3.5",
    "body-parser": "^1.18.3",
